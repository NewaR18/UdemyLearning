@model Company
<form class="mb-3">
    <div>
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name" type="text" class="form-control" readonly/>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div>
        <label asp-for="StreetAddress" class="form-label"></label>
        <input asp-for="StreetAddress" type="text" class="form-control" readonly />
        <span asp-validation-for="StreetAddress" class="text-danger"></span>
    </div>
    <div>
        <label asp-for="City" class="form-label"></label>
        <input asp-for="City" type="text" class="form-control" readonly />
        <span asp-validation-for="City" class="text-danger"></span>
    </div>
    <div>
        <label asp-for="State" class="form-label"></label>
        <input asp-for="State" type="text" class="form-control" readonly />
        <span asp-validation-for="State" class="text-danger"></span>
    </div>
    <div>
        <label asp-for="PostalCode" class="form-label"></label>
        <input asp-for="PostalCode" type="text" class="form-control" readonly />
        <span asp-validation-for="PostalCode" class="text-danger"></span>
    </div>
    <div>
        <label asp-for="PhoneNumber" class="form-label"></label>
        <input asp-for="PhoneNumber" type="text" class="form-control" readonly />
        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
    </div>
</form>
<a class="bg-primary text-light p-2 text-decoration-none mt-3" asp-controller="Company" asp-action="Index">Back To Index</a>
