@model Category
<form class="mb-3">
    <div>
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name" type="text" class="form-control" readonly/>
        <span asp-validation-for="Name"></span>
    </div>
    <div>
        <label asp-for="DisplayOrder" class="form-label"></label>
        <input asp-for="DisplayOrder" type="number" class="form-control" readonly />
        <span asp-validation-for="DisplayOrder" class="text-danger"></span>
    </div>
</form>
<a class="bg-primary text-light p-2 text-decoration-none mt-3" asp-controller="Category" asp-action="Index">Back To Index</a>
